import os
from xml.etree import ElementTree


doc = ElementTree.parse('exemple.xml')

racine = doc.getroot()

print(racine.tag)

titres = doc.find('TEI/teiHeader/fileDesc/titleStmt')
for t in titres:
    titre = t.find('title').text
    print(titre)

if __name__ == '__main__':
    pass