from xml.etree import ElementTree

doc = ElementTree.parse("exemple.xml")
racine = doc.getroot()

print(racine.tag)

for noeud in racine.getchildren():
    print(noeud.tag)
print("le fichier contient les noeuds suivants :")
for noeuds in racine.getiterator():
    print(noeuds.tag)

for titre in doc.findall("TEI/teiHeader/fileDesc/titleStmt/title.text"):
    print("titre: \n", titre.text)

if __name__ == '__main__':
    pass