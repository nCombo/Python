'''
Created on 5 nov. 2015

@author: combo
'''

import sqlite3


conn = sqlite3.connect('ma_base.db')

cursor = conn.cursor()
cursor.excute("""
CREATE TABLE IF NOT EXISTS users(
    id INTEGER PRIMARY KEY AUTOINCREMENT UNIQUE,
    name TEXT,
    age INTERGER
)
""")
conn.cmmit()

if __name__ == '__main__':
    pass