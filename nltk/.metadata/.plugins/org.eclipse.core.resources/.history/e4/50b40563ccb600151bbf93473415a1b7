import nltk
from matplotlib import use, pyplot
 use('agg')
 pyplot.savefig('modals.png') # we use pyplot.savefig() instead of pyplot.show() to save frequency
 print('Content-Type: text/html')
 print('<html><body>')
 print('<img src="modals.png"/>')
 print('</body></html>')
       